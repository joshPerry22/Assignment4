@{
    ViewBag.Title = "Waitlist";
}

@model IEnumerable<DateMe.Models.ApplicationResponse>
<div>
    <h2>Individuals currently on the wait list</h2>

    <table>
        <thead>
            <tr><th>Name</th><th>Age</th><th>Phone</th><th>Major</th><th>Occupation</th><th>Freak?</th></tr>
        </thead>
        <tbody>
            @foreach(DateMe.Models.ApplicationResponse x in Model)
            {
            <tr>
                <td>@x.name</td>
                <td>@x.age</td>
                <td>@x.phone</td>
                <td>@x.major</td>
                <td>@x.job</td>
                <td>@x.freak</td>
            </tr>
            }
        </tbody>
        
    </table>
</div>

